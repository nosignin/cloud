<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.redis.mapper.DictionaryMapper">
    <sql id="Base_Column_List">
        id,`key`,`value`,status,description
    </sql>
    
    <select id="getKey" resultType="com.example.redis.entity.Dictionary">
        select <include refid="Base_Column_List"/> from dictionary where `key` = #{key,jdbcType=VARCHAR}
    </select>
</mapper>